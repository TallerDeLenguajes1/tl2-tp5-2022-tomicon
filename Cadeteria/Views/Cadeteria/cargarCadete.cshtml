    <title>cargar Cadete</title>
</head>
<body>
    <header>
        <h1>Carga de Cadetes</h1>
    </header>
    <main>
        <form asp-controller="Cadeteria" asp-action="AltaCadete" method="POST">
            <label for="nombCadete">Nombre: </label>
            <input type="text" id="nombCadete" name="nombCadete" placeholder="Nombre">
            <label for="apeCadete">Apellido: </label>
            <input type="text" id="apeCadete" name="apeCadete" placeholder="Apellido">
            <label for="telCadete">Telefono: </label>
            <input type="tel" id="telCadete" name="telCadete" placeholder="3811111111">
            <input type="submit" value="Cargar Cadete">
        </form>
    </main>
</body>
</html>
